#include <iostream>

using namespace std;

int main()
{
    int m,n;
    cin>>m>>n;
    
    for (int a=0;a<m;a++){
        for (int b=0;b<n;b++){
            if (a==0||a==m-1||b==0||b==n-1)
                cout<<"* ";
            else
                cout<<"  ";
        }
        cout<<endl;
    }

    return 0;
}
